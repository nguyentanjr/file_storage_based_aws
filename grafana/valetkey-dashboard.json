{
  "annotations": { "list": [] },
  "__inputs": [
    {
      "name": "DS_CLOUDWATCH",
      "label": "CloudWatch",
      "type": "datasource",
      "pluginId": "cloudwatch",
      "pluginName": "CloudWatch"
    }
  ],
  "schemaVersion": 36,
  "version": 1,
  "title": "ValetKey CloudWatch Dashboard (Fixed)",
  "refresh": "30s",
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "type": "row",
      "title": "Overview",
      "gridPos": { "x": 0, "y": 0, "w": 24, "h": 1 }
    },

    {
      "id": 10,
      "type": "stat",
      "title": "EC2 CPU (%) - i-0afa4320b0c85245f",
      "datasource": "${DS_CLOUDWATCH}",
      "targets": [
        {
          "namespace": "AWS/EC2",
          "metricName": "CPUUtilization",
          "statistics": ["Average"],
          "period": "60",
          "region": "ap-southeast-1",
          "dimensions": { "InstanceId": "i-0afa4320b0c85245f" },
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "percent" } },
      "gridPos": { "x": 0, "y": 1, "w": 6, "h": 4 }
    },
    {
      "id": 11,
      "type": "stat",
      "title": "EC2 Network In (Bytes/s)",
      "datasource": "${DS_CLOUDWATCH}",
      "targets": [
        {
          "namespace": "AWS/EC2",
          "metricName": "NetworkIn",
          "statistics": ["Average"],
          "period": "60",
          "region": "ap-southeast-1",
          "dimensions": { "InstanceId": "i-0afa4320b0c85245f" },
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "bytesps" } },
      "gridPos": { "x": 6, "y": 1, "w": 6, "h": 4 }
    },
    {
      "id": 12,
      "type": "stat",
      "title": "EC2 Network Out (Bytes/s)",
      "datasource": "${DS_CLOUDWATCH}",
      "targets": [
        {
          "namespace": "AWS/EC2",
          "metricName": "NetworkOut",
          "statistics": ["Average"],
          "period": "60",
          "region": "ap-southeast-1",
          "dimensions": { "InstanceId": "i-0afa4320b0c85245f" },
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "bytesps" } },
      "gridPos": { "x": 12, "y": 1, "w": 6, "h": 4 }
    },
    {
      "id": 13,
      "type": "stat",
      "title": "EC2 Disk Read Ops",
      "datasource": "${DS_CLOUDWATCH}",
      "targets": [
        {
          "namespace": "AWS/EC2",
          "metricName": "DiskReadOps",
          "statistics": ["Sum"],
          "period": "60",
          "region": "ap-southeast-1",
          "dimensions": { "InstanceId": "i-0afa4320b0c85245f" },
          "refId": "A"
        }
      ],
      "gridPos": { "x": 18, "y": 1, "w": 6, "h": 4 }
    },

    {
      "id": 20,
      "type": "graph",
      "title": "Lambda Performance - valetkey-backup-worker",
      "datasource": "${DS_CLOUDWATCH}",
      "targets": [
        {
          "namespace": "AWS/Lambda",
          "metricName": "Invocations",
          "statistics": ["Sum"],
          "period": "60",
          "region": "ap-southeast-1",
          "dimensions": { "FunctionName": "valetkey-backup-worker" },
          "refId": "A",
          "alias": "Invocations"
        },
        {
          "namespace": "AWS/Lambda",
          "metricName": "Errors",
          "statistics": ["Sum"],
          "period": "60",
          "region": "ap-southeast-1",
          "dimensions": { "FunctionName": "valetkey-backup-worker" },
          "refId": "B",
          "alias": "Errors"
        },
        {
          "namespace": "AWS/Lambda",
          "metricName": "Duration",
          "statistics": ["p95"],
          "period": "60",
          "region": "ap-southeast-1",
          "dimensions": { "FunctionName": "valetkey-backup-worker" },
          "refId": "C",
          "alias": "Duration (p95 ms)"
        }
      ],
      "gridPos": { "x": 0, "y": 5, "w": 12, "h": 8 }
    },

    {
      "id": 21,
      "type": "graph",
      "title": "SQS Queue - valetkey-backup-queue",
      "datasource": "${DS_CLOUDWATCH}",
      "targets": [
        {
          "namespace": "AWS/SQS",
          "metricName": "ApproximateNumberOfMessagesVisible",
          "statistics": ["Average"],
          "period": "60",
          "region": "ap-southeast-1",
          "dimensions": { "QueueName": "valetkey-backup-queue" },
          "refId": "A",
          "alias": "Visible Messages"
        },
        {
          "namespace": "AWS/SQS",
          "metricName": "ApproximateAgeOfOldestMessage",
          "statistics": ["Maximum"],
          "period": "60",
          "region": "ap-southeast-1",
          "dimensions": { "QueueName": "valetkey-backup-queue" },
          "refId": "B",
          "alias": "Oldest Age (s)"
        }
      ],
      "gridPos": { "x": 12, "y": 5, "w": 12, "h": 8 }
    },

    {
      "id": 30,
      "type": "graph",
      "title": "Backup Metrics Over Time (ValetKey/Backup)",
      "datasource": "${DS_CLOUDWATCH}",
      "targets": [
        {
          "namespace": "ValetKey/Backup",
          "metricName": "BackupLatency",
          "statistics": ["Average"],
          "period": "300",
          "region": "ap-southeast-1",
          "dimensions": {},
          "refId": "A",
          "alias": "Avg Latency"
        },
        {
          "namespace": "ValetKey/Backup",
          "metricName": "BackupThroughput",
          "statistics": ["Average"],
          "period": "300",
          "region": "ap-southeast-1",
          "dimensions": {},
          "refId": "B",
          "alias": "Throughput (bytes/s)"
        }
      ],
      "gridPos": { "x": 0, "y": 13, "w": 12, "h": 8 }
    },

    {
      "id": 31,
      "type": "graph",
      "title": "Backup Success Counts (COMPLETED / FAILED / PENDING)",
      "datasource": "${DS_CLOUDWATCH}",
      "targets": [
        {
          "namespace": "ValetKey/Backup",
          "metricName": "BackupSuccess",
          "statistics": ["Sum"],
          "period": "300",
          "region": "ap-southeast-1",
          "dimensions": { "Status": "COMPLETED" },
          "refId": "A",
          "alias": "COMPLETED"
        },
        {
          "namespace": "ValetKey/Backup",
          "metricName": "BackupSuccess",
          "statistics": ["Sum"],
          "period": "300",
          "region": "ap-southeast-1",
          "dimensions": { "Status": "FAILED" },
          "refId": "B",
          "alias": "FAILED"
        },
        {
          "namespace": "ValetKey/Backup",
          "metricName": "BackupSuccess",
          "statistics": ["Sum"],
          "period": "300",
          "region": "ap-southeast-1",
          "dimensions": { "Status": "PENDING" },
          "refId": "C",
          "alias": "PENDING"
        }
      ],
      "gridPos": { "x": 12, "y": 13, "w": 12, "h": 8 }
    },

    {
      "id": 40,
      "type": "graph",
      "title": "RDS - valetkey-db (CPU & Connections)",
      "datasource": "${DS_CLOUDWATCH}",
      "targets": [
        {
          "namespace": "AWS/RDS",
          "metricName": "CPUUtilization",
          "statistics": ["Average"],
          "period": "300",
          "region": "ap-southeast-1",
          "dimensions": { "DBInstanceIdentifier": "valetkey-db" },
          "refId": "A",
          "alias": "CPU (%)"
        },
        {
          "namespace": "AWS/RDS",
          "metricName": "DatabaseConnections",
          "statistics": ["Average"],
          "period": "300",
          "region": "ap-southeast-1",
          "dimensions": { "DBInstanceIdentifier": "valetkey-db" },
          "refId": "B",
          "alias": "DB Connections"
        },
        {
          "namespace": "AWS/RDS",
          "metricName": "FreeStorageSpace",
          "statistics": ["Average"],
          "period": "300",
          "region": "ap-southeast-1",
          "dimensions": { "DBInstanceIdentifier": "valetkey-db" },
          "refId": "C",
          "alias": "Free Storage (bytes)"
        }
      ],
      "gridPos": { "x": 0, "y": 21, "w": 12, "h": 8 }
    },

    {
      "id": 50,
      "type": "graph",
      "title": "S3 - aws-cloud-int3319x (Bucket Size & Objects)",
      "datasource": "${DS_CLOUDWATCH}",
      "targets": [
        {
          "namespace": "AWS/S3",
          "metricName": "BucketSizeBytes",
          "statistics": ["Average"],
          "period": "86400",
          "region": "ap-southeast-1",
          "dimensions": { "BucketName": "aws-cloud-int3319x", "StorageType": "StandardStorage" },
          "refId": "A",
          "alias": "Bucket Size (bytes)"
        },
        {
          "namespace": "AWS/S3",
          "metricName": "NumberOfObjects",
          "statistics": ["Average"],
          "period": "86400",
          "region": "ap-southeast-1",
          "dimensions": { "BucketName": "aws-cloud-int3319x", "StorageType": "AllStorageTypes" },
          "refId": "B",
          "alias": "Number of Objects"
        }
      ],
      "gridPos": { "x": 12, "y": 21, "w": 12, "h": 8 }
    },

    {
      "id": 60,
      "type": "logs",
      "title": "Recent Errors (CloudWatch Logs Insights)",
      "datasource": "${DS_CLOUDWATCH}",
      "options": {
        "query": "fields @timestamp, @message\n| filter @message like /ERROR|Exception|Error/\n| sort @timestamp desc\n| limit 50",
        "queryType": "logs"
      },
      "gridPos": { "x": 0, "y": 29, "w": 24, "h": 8 }
    }
  ],
  "time": { "from": "now-6h", "to": "now" },
  "templating": {
    "list": [
      {
        "type": "datasource",
        "name": "DS_CLOUDWATCH",
        "pluginId": "cloudwatch",
        "label": "CloudWatch",
        "query": "cloudwatch"
      },
      {
        "type": "constant",
        "name": "EC2_INSTANCE",
        "label": "EC2 instance",
        "query": "i-0afa4320b0c85245f",
        "hide": 1
      }
    ]
  }
}
